    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ medicament.id }}</td>
            </tr>
            <tr>
                <th>Nomcommercial</th>
                <td>{{ medicament.nomcommercial }}</td>
            </tr>
